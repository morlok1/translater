{
    "manifest_version": 2,
 
    "name": "Translate",
    "description": "For translate",
    "version": "0.1",
 
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
	
	
	"permissions": [
		"https://dictionary.yandex.net/*"
	],

    "permissions": [
        "activeTab"
    ],
	
	  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl + Space"
      }
    }
  }
}